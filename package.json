    "build:frontend": "wp-scripts build front/build/index.js --output-path=front/build",
    "build:backend": "wp-scripts build build/index.js --output-path=build",
    "test:e2e": "wp-scripts test-e2e",
    "test:unit": "wp-scripts test-unit-js"
  },
